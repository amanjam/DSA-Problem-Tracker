{
  "manifest_version": 3,
  "name": "DSA Problem Tracker",
  "version": "0.1",
  "author": "Aman Phogat",
  "description": "we will be bookmarking the DSA problems and making notes for that also.",
  "permissions": [
    "activeTab",
    "storage"

  ],
  "content_scripts": [
    {
      "matches": [
        "https://maang.in/problems/*"
      ],
      "js": [
        "content.js"
      ]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources" : [
    {
      "resources": ["Starter Files/assets/ext-icon.png", "Starter Files/assets/play.png", "Starter Files/styles/delete.css"],
      "matches" : ["<all_urls>"]
    }
  ],
  "action": {
    "default_icon": {
      "16": "Starter Files/assets/ext-icon.png",
      "24": "Starter Files/assets/ext-icon.png",
      "32": "Starter Files/assets/ext-icon.png"
    },
    "default_popup": "index.html",
    "default_title": "DSA Problem Tracker"
  }
}
